   xsi:schemaLocation="
        http://xmlns.jcp.org/xml/ns/persistence
        http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
   <persistence-unit name="WFTravel">
      <properties>
         <!-- Properties -->
         <property name="openjpa.ConnectionUserName" value="test_user" />
